      "tags": "高质量,中文渲染",
      "sort_order": 10,
      "is_active": true,
      "is_recommended": true,
      "third_party_model_id": "Tencent-Hunyuan/HunyuanImage-3.0",
      "base_url": "https://api-inference.modelscope.cn/",
      "default_width": 1024,
      "default_height": 1024,
      "max_width": 2048,
      "max_height": 2048,
      "min_steps": 15,
      "max_steps": 50
    },
    {
      "model_id": "flux.1-dev",
      "model_name": "FLUX.1-dev",
      "model_type": "text2img",
      "provider": "modelscope",
      "description": "FLUX.1 [dev] 是一个具有 120 亿参数的修正流变换器，能够根据文本描述生成图像",
      "tags": "英文渲染",
      "sort_order": 20,
      "is_active": true,
      "is_recommended": false,
